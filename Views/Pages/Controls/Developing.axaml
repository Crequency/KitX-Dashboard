<UserControl x:Class="KitX_Dashboard.Views.Pages.Controls.Developing"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:ui="using:FluentAvalonia.UI.Controls"
             d:DesignHeight="450"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
        <TextBlock Margin="0,5,0,20"
                   HorizontalAlignment="Center"
                   FontFamily="{StaticResource SourceHanSans}"
                   FontSize="24"
                   FontWeight="Bold"
                   Text="{DynamicResource Text_Public_Developing}"/>
        <ui:HyperlinkButton HorizontalAlignment="Center"
                            Content="github.com/Crequency/KitX"
                            FontFamily="{StaticResource SourceHanSans}"
                            FontSize="14"
                            NavigateUri="https://github.com/Crequency/KitX"/>
    </StackPanel>
</UserControl>
